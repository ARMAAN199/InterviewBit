public class Solution {
    public int solve(int[] A) {
            int min = Integer.MAX_VALUE;
            int max = Integer.MIN_VALUE;
        for(int i=0; i<A.length; i++)
        {
            if(A[i]<min) min = A[i];
            if(A[i]>max) max = A[i];
        }
        return min+max;
    }
}
